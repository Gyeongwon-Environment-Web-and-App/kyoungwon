{
  "name": "frontend-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "vercel-build": "npm run build",
    "eject": "react-scripts eject",
    "cap:sync": "npx cap sync",
    "cap:open:ios": "npx cap open ios",
    "cap:open:android": "npx cap open android",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lighthouse": "lighthouse http://localhost:5173 --config-path ./lighthouse.config.js --output html --output-path ./lighthouse-report.html --chrome-flags=\"--headless\"",
    "lighthouse:mobile": "lighthouse http://localhost:5173 --config-path ./lighthouse.config.js --output html --output-path ./lighthouse-mobile-report.html --chrome-flags=\"--headless\" --form-factor=mobile --throttling-method=devtools",
    "perf:test": "npm run build && npm run preview & sleep 5 && npm run lighthouse && npm run lighthouse:mobile && pkill -f \"vite preview\"",
    "build:android": "npm run build && npx cap sync android && npx cap build android",
    "build:ios": "npm run build && npx cap sync ios && npx cap build ios",
    "build:release:android": "npm run build && npx cap sync android && cd android && ./gradlew assembleRelease",
    "build:release:ios": "npm run build && npx cap sync ios && npx cap build ios --prod"
  },
  "dependencies": {
    "@capacitor/android": "^7.4.2",
    "@capacitor/camera": "^7.0.2",
    "@capacitor/cli": "^7.4.2",
    "@capacitor/core": "^7.4.2",
    "@capacitor/ios": "^7.4.2",
    "@capacitor/preferences": "^7.0.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/vite": "^4.1.11",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.525.0",
    "react": "^19.1.1",
    "react-day-picker": "^9.11.0",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.6.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "vercel": "^48.1.6",
    "xlsx": "^0.18.5",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/xlsx": "^0.0.35",
    "@vitejs/plugin-react": "^4.5.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "lighthouse": "^12.8.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "tailwindcss": "^3.4.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^7.0.0"
  }
}
